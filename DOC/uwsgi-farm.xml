<uwsgi>
        <socket>/var/run/uwsgi/uwsgi-farm.sock</socket>
        <process>1</process>

        <master/>
        <enable-threads/>

        <uid>www</uid>
        <gid>www</gid>
        <callable>application</callable>
        <chunked-input-timeout>120</chunked-input-timeout>
        <buffer-size>64000</buffer-size>
        <pidfile>/var/run/uwsgi-farm.pid</pidfile>
</uwsgi>
